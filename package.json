{
  "name": "js-weapon",
  "version": "0.0.3",
  "author": "清扬陌客",
  "description": "js 武器库",
  "keywords": [
    "js",
    "utils",
    "tools"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qingyangmoke/js-weapon.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "dtsmake": "dtsmake -s ./src/array/arrayEqual.js --dist ./types/arrayEqual.d.ts",
    "dev": "NODE_ENV=development webpack --progress --colors --config ./config/webpack.dev.config.js",
    "build": "node script/build.js",
    "build-webpack": "webpack --progress --colors --config ./config/webpack.config.js",
    "doc": "rm -rf docs && jsdoc README.md src -r -d docs -t node_modules/docdash",
    "lint": "eslint src/** tests/**",
    "test": "karma start ./config/karma.conf.js",
    "publish": "npm run build && npm publish dist"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^6.3.1",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.0",
    "copy": "^0.3.1",
    "docdash": "^0.4.0",
    "dtsmake": "^0.0.10",
    "eslint": "~3.5.0",
    "eslint-config-standard": "~6.0.1",
    "eslint-plugin-promise": "~3.4.0",
    "eslint-plugin-standard": "~2.0.1",
    "floss": "^2.0.1",
    "jasmine-core": "^2.8.0",
    "jsdoc": "~3.4.2",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-power-assert": "^1.0.0",
    "karma-webpack": "^2.0.7",
    "mocha": "^4.0.1",
    "ora": "^1.3.0",
    "power-assert": "^1.4.4",
    "promise": "^8.0.1",
    "rimraf": "^2.6.2",
    "string-replace-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "~0.15.0"
  },
  "types": "types/index.d.ts",
  "bugs": {
    "url": "https://github.com/qingyangmoke/js-weapon/issues"
  },
  "homepage": "https://github.com/qingyangmoke/js-weapon#readme"
